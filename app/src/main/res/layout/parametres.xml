<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".parametres">


    <Space
        android:layout_width="match_parent"
        android:layout_height="20dp"></Space>

    <TextView
        android:labelFor="@+id/TV_connexion"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:layout_marginTop="25dp"
        android:text="ParamÃ¨tres"
        android:textAppearance="@style/TextAppearance.Material3.BodyLarge"
        android:textColor="#0D3C0D"
        android:textSize="50dp"
        />

    <Space
        android:layout_width="match_parent"
        android:layout_height="40dp"></Space>

    <include layout="@layout/entete"/>

    <Space
        android:layout_width="match_parent"
        android:layout_height="40dp"></Space>


    <TextView
        android:labelFor="@+id/TV_E_mail3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:padding="10dp"
        android:text="E-mail"
        android:textColor="#243103"
        android:textSize="19dp" />
    <EditText
        android:id="@+id/EMAIL"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:padding="4dp"
        android:background="#E9FDB7"
        android:hint=""
        android:textColor="#CCEA7D" />
    <TextView
        android:labelFor="@+id/codev"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:padding="10dp"
        android:text="Code Visiteur :"
        android:textColor="#243103"
        android:textSize="19dp" />
    <EditText
        android:id="@+id/CODEV"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:padding="4dp"
        android:background="#E9FDB7"
        android:hint=""
        android:textColor="#CCEA7D" />

    <TextView
        android:labelFor="@+id/TV_Nom2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:padding="10dp"
        android:text="Nom :"
        android:textColor="#243103"
        android:textSize="19dp" />
    <EditText
        android:id="@+id/NOM"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:padding="4dp"
        android:background="#E9FDB7"
        android:hint=""
        android:textColor="#CCEA7D" />
    <TextView
        android:labelFor="@+id/TV_Prenom2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:padding="10dp"
        android:text="Prenom :"
        android:textColor="#243103"
        android:textSize="19dp" />
    <EditText
        android:id="@+id/PRENOM"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:padding="10dp"
        android:background="#E9FDB7"
        android:hint=""
        android:textColor="#CCEA7D" />
    <TextView
        android:labelFor="@+id/TV_Url"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:padding="10dp"
        android:text="Url"
        android:textColor="#243103"
        android:textSize="19dp" />
    <EditText
        android:id="@+id/URLSERVEUR"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:padding="4dp"
        android:background="#E9FDB7"
        android:hint=""
        android:textColor="#CCEA7D" />



    <Button
        android:id="@+id/main_button_11"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:backgroundTint="#1eb300"
        android:textColor="#233023"
        app:cornerRadius="5dp"
        android:layout_margin="3dp"
        android:text="Confirmer"
        android:onClick="MonClickParam"/>




</LinearLayout>

